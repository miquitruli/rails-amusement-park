<% @attractions.each do |attraction| %>
  <% if current_user.admin == false %>
    <p><%= link_to "Go on #{attraction.name}", attraction_path(attraction) %></p>
  <% else %>
    <p><%= link_to "Show #{attraction.name}", attraction_path(attraction) %></>
  <% end %>
<% end %>
 <% if current_user.admin == true %>
  <%= link_to "New Attraction", new_attraction_path %>
<% end %>